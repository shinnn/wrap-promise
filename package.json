{
  "name": "wrap-promise",
  "version": "1.0.0",
  "description": "Like new Promise(), but prevents implicit rejection",
  "repository": "shinnn/wrap-promise",
  "author": {
    "name": "Shinnosuke Watanabe",
    "url": "https://github.com/shinnn"
  },
  "scripts": {
    "pretest": "eslint --config node_modules/@shinnn/eslintrc/rc.json --ignore-path .gitignore .",
    "test": "node test.js | tap-spec",
    "coverage": "istanbul cover test.js",
    "coveralls": "${npm_package_scripts_coverage} && istanbul-coveralls"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/shinnn/wrap-promise/blob/master/LICENSE"
    }
  ],
  "main": "wrap-promise-cjs.js",
  "files": [
    "no-fallback.js",
    "wrap-promise-cjs.js"
  ],
  "keywords": [
    "promise",
    "constructor",
    "then",
    "thenable",
    "throw",
    "error",
    "wrapper",
    "explicit",
    "browser",
    "client-side"
  ],
  "dependencies": {
    "es6-promise": "^2.1.1"
  },
  "devDependencies": {
    "@shinnn/eslintrc": "^1.0.0",
    "eslint": "^0.21.0",
    "istanbul": "^0.3.13",
    "istanbul-coveralls": "^1.0.2",
    "require-uncached": "^1.0.2",
    "tap-spec": "^3.0.0",
    "tape": "^4.0.0"
  }
}
